
#include<stdio.h>
int main()
{
	int a[100][100];
	int i,j,r,c,sum;
	printf("enter the no.of rows");
	scanf("%d",&r);
	printf("enter the no of coloums");
	scanf("%d",&c);
	printf("enterthe elements into array");
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	for(i=0;i<r;i++)
	{
		sum=0;
		for(j=0;j<c;j++)
		{
			sum=sum+a[i][j];
		}
		printf("%d row sun=%d\n,i+1,sum");
	}
}
