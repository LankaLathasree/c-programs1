
int main()
{
	int a[100][100];
	int i,j,r,c,sum;
	printf("enter the no.of rows");
	}
	for(i=0;i<r;i++)
	{
	scanf("%d",&r);
	printf("enter the no of coloums");
	scanf("%d",&c);
	printf("enterthe elements into array");
	for(i=0;i<r;i++)
	{
	
	
		for(j=0;j<c;j++)
		{
			scanf("%d",&a[i][j]);
		}
		sum=0;
		for(j=0;j<c;j++)
		{
			sum=sum+a[i][i];
		}
		printf("%d coloum sum=%d\n",i+1,sum);
	}
}
