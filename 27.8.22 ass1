
#include<stdio.h>
#include<string.h>
int main()
{
	char a1[20];
	char a2[20];
	int n1,n2,i,j,count,temp;
	printf("Enter the string");
	gets(a1);
	printf("Enter the value of string 2");
	gets(a2);
	n1=strlen(a1);
	n2=strlen(a2);
	if(n1==n2)
	{
	temp=n1;
	for(i=0;i<temp;i++)
	 { 
		for(j=0;j<temp;j++)
		{
			if(a1[i]==a2[j])
			{
				count++;
			}
		}
	 }	
	}
	if(count==n1)
	{
		printf("ANNAGRAM");
	}
	else
	{
		printf("NOT AN ANNAGRAM");
}
	return 0;
}
