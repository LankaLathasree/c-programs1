
#include<stdio.h>
#include<stdlib.h>
int	main()
{
	int	*p,sum=0,n,i;
	puts("enter	te	value	of	n");
	scanf("%d",&n);
	p=(int*)malloc(n*sizeof(int));
	puts("enter	the	elements");
	for(i=0;i<n;i++)
	scanf("%d",p+i);
	
	for(i=0;i<n;i++)
	{
		sum=sum+*(p+i);
	}
	puts("sum	of	the	elements	is:");
	printf("%d",sum);
	return	0;
}
