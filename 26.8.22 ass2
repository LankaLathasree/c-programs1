
#include<stdio.h>
#include<string.h>
int main()
{
	char str[10];
	printf("enter the string");
	gets(str);
	int size;
	 size=strlen(str);
	int i=0,j=size-1;
	while(i<j/2)
	{
		if(str[i]==str[j])
		{
				printf("given sting is palendrome");
		 i++;
		 j--;
		}
		else
		{
	            printf("given string is not palendrome");
			i++;
			j--;
		}
	}
	puts(str);

	return 0;
}
